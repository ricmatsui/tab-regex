{
  "manifest_version": 2,

  "name": "Tab Regex",
  "description": "Renaming tabs",
  "version": "0.1",

  "permissions": [
    "storage",
    "tabs",
    "*://*/*"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["page.js"]
    }
  ],
  "options_page": "options.html"
}