{
  "manifest_version": 2,

  "name": "Tab Regex",
  "description": "Renaming tabs",
  "version": "0.1",

  "permissions": [
    "tabs",
    "*://*/*"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}